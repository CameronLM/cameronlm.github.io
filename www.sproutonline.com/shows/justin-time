            {{each(index, value) BBV.utils.getArray(BBV.utils.getEndngPageNumber(1, totalResults, 4) + 1)}}
	            {{if index >= 1}}
		            {{if index == 1}}
			            <span class="current">${index}</span>
		            {{else}}
			            <a href="https://web.archive.org/web/20120904080115/http://www.sproutonline.com/shows/justin-time?page=${index}&active=videos#videos">${index}</a>
		            {{/if}}
		
		            {{if index != BBV.utils.getEndngPageNumber(1, totalResults, 4)}}
			            <span>|</span> 
		            {{/if}}
	            {{/if}}
            {{/each}}
        
            {{if BBV.utils.hasEndingEllipses(1, totalResults, 4) }}
                <span>|</span>...<span>|</span> <a href="https://web.archive.org/web/20120904080115/http://www.sproutonline.com/shows/justin-time?page=${BBV.utils.getTotalPages(totalResults, 4)}&active=videos#videos">${BBV.utils.getTotalPages(totalResults, 4)}</a>
            {{/if}}
            {{if 1 != BBV.utils.getTotalPages(totalResults, 4) }}
                <a href="https://web.archive.org/web/20120904080115/http://www.sproutonline.com/shows/justin-time?page=2&active=videos#videos" class="next">&rsaquo;</a>
            {{/if}}
        </div>

        <div class="mm-results">
            <em>1-{{if 4 > totalResults}}${ totalResults } {{else}}4{{/if}} of ${ totalResults }</em> Results
        </div>
</script>


