            <em>Page</em> 
            {{if 1 != 1 }}<a href="https://web.archive.org/web/20120908083217/http://www.sproutonline.com/shows/angelina-ballerina?page=0&active=videos#videos" class="previous">&lsaquo;</a>{{/if}}
            {{if false }}<a href="https://web.archive.org/web/20120908083217/http://www.sproutonline.com/shows/angelina-ballerina?page=1&active=videos#videos">1</a><span>|</span>...<span>|</span>{{/if}}
        
            {{each(index, value) BBV.utils.getArray(BBV.utils.getEndngPageNumber(1, totalResults, 4) + 1)}}
	            {{if index >= 1}}
		            {{if index == 1}}
			            <span class="current">${index}</span>
		            {{else}}
			            <a href="https://web.archive.org/web/20120908083217/http://www.sproutonline.com/shows/angelina-ballerina?page=${index}&active=videos#videos">${index}</a>
		            {{/if}}
		
		            {{if index != BBV.utils.getEndngPageNumber(1, totalResults, 4)}}
			            <span>|</span> 
		            {{/if}}
	            {{/if}}
            {{/each}}
        
            {{if BBV.utils.hasEndingEllipses(1, totalResults, 4) }}
                <span>|</span>...<span>|</span> <a href="https://web.archive.org/web/20120908083217/http://www.sproutonline.com/shows/angelina-ballerina?page=${BBV.utils.getTotalPages(totalResults, 4)}&active=videos#videos">${BBV.utils.getTotalPages(totalResults, 4)}</a>
            {{/if}}
            {{if 1 != BBV.utils.getTotalPages(totalResults, 4) }}
                <a href="https://web.archive.org/web/20120908083217/http://www.sproutonline.com/shows/angelina-ballerina?page=2&active=videos#videos" class="next">&rsaquo;</a>
            {{/if}}
        </div>

        <div class="mm-results">
            <em>1-{{if 4 > totalResults}}${ totalResults } {{else}}4{{/if}} of ${ totalResults }</em> Results
        </div>
</script>


