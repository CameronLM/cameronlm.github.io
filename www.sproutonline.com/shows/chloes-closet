        <script type="text/javascript">

            var kidSpace = kidSpace || {};

            kidSpace.init = function () {

                var $kidSpace = $('#kidsSpace'),
                $kidSpacePromo = $('#kids-space-promo');

                function kidSpaceShow() {
                    $kidSpace.addClass("min");
                    $kidSpace.css("height", "90px");
                    $kidSpacePromo.css("display", "none");
                    $('#flash-wrap').css("display", "none");
                }

                function kidSpaceHide() {
                    var flashvars = { outgoingURL: document.getElementById('kids-space-link').getAttribute('href') },
                    swfFileName = $kidSpacePromo.attr("data-swf-file");
                    params = { menu: 'false', scale: 'noScale', allowFullscreen: 'true', allowScriptAccess: 'always', wmode: 'transparent' },
                    attributes = { id: 'sprout' };

                    $kidSpace.css({
                        overflow: "hidden",
                        height: "424px"
                    });

                    $kidSpacePromo.css("display", "block");

                    swfobject.embedSWF(swfFileName, 'kids-space-promo', '1100px', '100%', '10.0.0', '/swf/expressInstall.swf', flashvars, params, attributes);
                }

                if (document.cookie.indexOf('KidsSpaceShown') != -1) {
                    kidSpaceShow();
                } else {
                    kidSpaceHide();
                }
            }

            kidSpace.init();
        </script>
