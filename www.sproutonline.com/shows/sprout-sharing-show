            {{each(index, value) BBV.utils.getArray(BBV.utils.getEndngPageNumber(1, totalResults, 4) + 1)}}
	            {{if index >= 1}}
		            {{if index == 1}}
			            <span class="current">${index}</span>
		            {{else}}
			            <a href="https://web.archive.org/web/20120906085830/http://www.sproutonline.com/shows/sprout-sharing-show?page=${index}&active=videos#videos">${index}</a>
