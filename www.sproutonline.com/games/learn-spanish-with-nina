Omni.Doc.Content.Content("games","Good Night Show","Learn Spanish with Nina","",true,null);
Omni.Doc.Content.Asset.Asset();
Omni.Doc.Content.Asset.Game("","GameDifficultyLevel_2");

                var concatEventList = Omni.events.concat(Omni.Game.e);
                var concatPropList = Omni.properties.concat(Omni.Game.p);
                Omni.EventList.add(2, concatEventList);
                Omni.Send(concatEventList, concatPropList);
                $('.sproutlets-btn.playnow').die();
            } catch (error) {
                // Error tracking "Play Now" click
            }
        });
    </script>



    
