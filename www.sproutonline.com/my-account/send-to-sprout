    <script type="text/javascript">
        $(document).ready(function () {
            frm.validate();
            Sprout.SendToSprout.SproutletCreations.InfoBox();
            Sprout.SendToSprout.loadAllForms(1);
            Sprout.SendToSprout.setUploadType();
            Sprout.SendToSprout.SproutletCreations.Load(1);
            
            //add required spans to format.
            var fieldsets = $('.upload-steps-wrap').find('fieldset');
            $.each(fieldsets, function (index, val) {
                if (index === 1) {
                    $(val).before('<span class="step-num-2">2</span>');
                }
                else if (index === 2) {
                    $(val).wrap('<div class="fieldWrap"/>').first().before('<span class="step-num-3">3</span>');
                }
            });

            $('.drpFormList').change(function () {
                if (this.selectedIndex == 0) {
                    window.location.replace('https://web.archive.org/web/20120910000124/http://www.sproutonline.com/my-account/send-to-sprout');
                    return;
                }
                var Url = 'https://web.archive.org/web/20120910000124/http://www.sproutonline.com/my-account/send-to-sprout' + '?formGuid=' + $(this).find('option:selected').first().attr('value');
                window.location.replace(Url);
            });

            var $birthdayField = $(".sproutletsbirthdate");

            if ($birthdayField.size() > 0) {
                $(".sproutletsbirthdate input[type='text']").datepicker();
            }

            Sprout.SendToSprout.CheckSubmission();

        });

        function pageLoad() {
            Sprout.SendToSprout.setValues([]);
        }
    </script>
