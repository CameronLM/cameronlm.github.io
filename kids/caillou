            (function ($, swfobject) {
                var flashvars = {},
			        params = {
			            menu: "false",
			            scale: "noScale",
			            allowFullscreen: "true",
			            allowScriptAccess: "always",
			            bgcolor: "",
			            wmode: "transparent"
			        },
			        attributes = {
			            id: "sprout"
			        };


                function callback(evt) {
                    $('body').fadeIn();
                }

                function embed(evt) {
                    //document is ready populate flashvars
                    var dataElement = $('<div />', { "class": "data-container" }).append($('.flash-data'));
                    var dataString = innerXHTML(dataElement[0]);
                
                    flashvars.dataString = encodeURIComponent(dataString);

                    swfobject.embedSWF("/media/749523/010101000000/sprout.swf", "kids-flash-feature-inner", "1000px", "558px", "10.0.0", "/swf/expressInstall.swf", flashvars, params, attributes, callback);
                }

                (function init() {
                    $(document).ready(embed);
                })();
            })(jQuery, swfobject)
        
